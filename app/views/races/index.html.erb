<h1>log</h1>
<table>
<%= @runner.name %>
<% @runner.races.each do |race| %>
    <tr>
      <td><%= race.title %></td>
      <td><%= race.date %></td>
      <td><%= race.time %></td>
      <td><%= link_to 'edit', edit_runner_race_path(@runner, race) %></td>
    </tr>
  <% end %>
</table>
<%= link_to 'Jog Log', new_runner_race_path(@runner, @race) %>
